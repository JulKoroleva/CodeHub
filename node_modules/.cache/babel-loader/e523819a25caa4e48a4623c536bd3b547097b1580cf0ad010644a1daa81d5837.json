enderList($props.item.children, childItem => {\n    return _openBlock(), _createElementBlock(\"li\", {\n      key: childItem.name,\n      style: {\n        \"align-items\": \"flex-start\"\n      }\n    }, [_createVNode(_component_FileItem, {\n      item: childItem,\n      level: $props.level + 1,\n      newItemName: childItem.newItemName,\n      onUpdateNewItemName: $options.updateChildNewItemName\n    }, null, 8 /* PROPS */, [\"item\", \"level\", \"newItemName\", \"onUpdateNewItemName\"])]);\n  }), 128 /* KEYED_FRAGMENT */)) : _createCommentVNode(\"v-if\", true)])], 2 /* CLASS */)) : _createCommentVNode(\"v-if\", true)]);\n}","map":{"version":3,"names":["class","_createElementVNode","_hoisted_5","_hoisted_8","_createElementBlock","_hoisted_1","_hoisted_2","onDblclick","_cache","args","$options","renameItem","onClick","$event","toggleFolder","$props","item","onMouseover","showDownloadButton","onMouseleave","type","_hoisted_3","children","length","_hoisted_4","_hoisted_6","$data","expandedFolder","_hoisted_7","_hoisted_9","renaming","startRename","name","newName","onBlur","finishRename","onKeyup","_withKeys","ref","maxlength","selectFile","_withModifiers","closeCreateButtons","toggleCreateButtons","downloadFile","deleteFile","createMenuVisible","_hoisted_10","createFolder","style","disabled","level","_hoisted_11","createFile","showModal","_createBlock","_component_CreateFileModal","visible","prompt","modalPrompt","onConfirm","handleConfirmCreation","onCancel","handleCancel","itemType","modalType","onCreate","handleCreate","_ctx","collapsed","_normalizeClass","expand","_Fragment","key","_renderList","childItem","_createVNode","_component_FileItem","newItemName","onUpdateNewItemName","updateChildNewItemName"],"sources":["C:\\Users\\PC\\Desktop\\vue\\vue-code-collaborator\\src\\components\\FileItem.vue"],"sourcesContent":["<template>\r\n  <div class=\"repository-container\">\r\n    <div class=\"soso\"></div>\r\n    <div\r\n      class=\"control\"\r\n      @dblclick=\"renameItem\"\r\n      @click=\"toggleFolder(item)\"\r\n      @mouseover=\"showDownloadButton = true\"\r\n      @mouseleave=\"showDownloadButton = false\"\r\n    >\r\n      <span\r\n        v-if=\"item.type === 'folder' || item.type === 'array'\"\r\n        class=\"folder\"\r\n      >\r\n        <span\r\n          v-if=\"item.children && item.children.length > 0\"\r\n          class=\"folder-indicator\"\r\n          v-show=\"expandedFolder === item\"\r\n        >\r\n          <div class=\"arrow_down\"></div>\r\n        </span>\r\n        <span\r\n          v-if=\"item.children && item.children.length > 0\"\r\n          class=\"folder-indicator\"\r\n          v-show=\"expandedFolder !== item\"\r\n        >\r\n          <div class=\"arrow_right\"></div>\r\n        </span>\r\n        <span v-if=\"!item.renaming\" @dblclick=\"startRename\" class=\"name\">\r\n          {{ item.name }}\r\n        </span>\r\n        <input\r\n          v-else\r\n          type=\"text\"\r\n          v-model=\"item.newName\"\r\n          @blur=\"finishRename\"\r\n          @keyup.enter=\"finishRename\"\r\n          class=\"rename-input\"\r\n          ref=\"renameInput\"\r\n          maxlength=\"15\"\r\n        />\r\n      </span>\r\n\r\n      <span v-else @click=\"selectFile\" class=\"file\">\r\n        <span v-if=\"!item.renaming\" @dblclick=\"startRename\" class=\"name\">\r\n          {{ item.name }}\r\n        </span>\r\n        <input\r\n          v-else\r\n          type=\"text\"\r\n          v-model=\"item.newName\"\r\n          @blur=\"finishRename\"\r\n          @keyup.enter=\"finishRename\"\r\n          class=\"rename-input\"\r\n          ref=\"renameInput\"\r\n          maxlength=\"15\"\r\n        />\r\n      </span>\r\n      <div\r\n        class=\"download-btn\"\r\n        @click.prevent=\"toggleFolder(item)\"\r\n        @mouseleave=\"closeCreateButtons\"\r\n        v-show=\"showDownloadButton\"\r\n      >\r\n        <button\r\n          class=\"add_btn\"\r\n          @click=\"toggleCreateButtons(item)\"\r\n          v-if=\"item.type === 'folder'\"\r\n        ></button>\r\n        <button\r\n          class=\"add_btn\"\r\n          @click=\"toggleCreateButtons(item)\"\r\n          v-else-if=\"item.type === 'array'\"\r\n        ></button>\r\n        <button\r\n          class=\"download_btn\"\r\n          @click=\"downloadFile\"\r\n          v-if=\"item.type === 'text/plain'\"\r\n          v-show=\"showDownloadButton\"\r\n        ></button>\r\n        <button class=\"delete_btn\" @click=\"deleteFile(this.item)\"></button>\r\n\r\n        <div v-if=\"createMenuVisible\" class=\"create-buttons\">\r\n          <button\r\n  class=\"new-folder_btn\"\r\n  @click=\"createFolder\"\r\n  style=\"\r\n    background-color: transparent;\r\n    border: none;\r\n    border-bottom: 1px solid black;\r\n    color: #27282d;\r\n    opacity: this.item.level >= 6 ? 0.3 : 1;\r\n  \"\r\n  :disabled=\"this.item.level >= 6\"\r\n>\r\n  folder\r\n</button>\r\n\r\n          <button\r\n            class=\"new-file_btn\"\r\n            @click=\"createFile\"\r\n            style=\" background-color: rgba(197, 197, 197); border: none; color: #27282d\"\r\n          >\r\n            file\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <CreateFileModal\r\n      v-if=\"showModal\"\r\n      :visible=\"showModal\"\r\n      :prompt=\"modalPrompt\"\r\n      @confirm=\"handleConfirmCreation\"\r\n      @cancel=\"handleCancel\"\r\n      :itemType=\"modalType\"\r\n      @create=\"handleCreate\"\r\n      :level=\"level + 1\"\r\n    />\r\n\r\n    <div\r\n      v-if=\"!collapsed && item.children && item.children.length > 0\"\r\n      :class=\"{ expand: expandedFolder === item }\"\r\n      class=\"\"\r\n    >\r\n      <ul>\r\n        <li\r\n          v-for=\"childItem in item.children\"\r\n          :key=\"childItem.name\"\r\n          v-if=\"expandedFolder === item\"\r\n          style=\"align-items: flex-start\"\r\n        >\r\n          <FileItem\r\n            :item=\"childItem\"\r\n            :level=\"level + 1\"\r\n            :newItemName=\"childItem.newItemName\"\r\n            @updateNewItemName=\"updateChildNewItemName\"\r\n          />\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CreateFileModal from \"./CreateFileModal.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    CreateFileModal,\r\n  },\r\n  props: {\r\n    item: Object,\r\n    level: {\r\n      type: Number,\r\n      default: 0,\r\n    },\r\n    newItemName: String,\r\n  },\r\n  data() {\r\n    return {\r\n      showModal: false,\r\n      modalPrompt: \"\",\r\n      modalType: \"\",\r\n      createMenuVisible: false,\r\n      expandedFolder: null,\r\n      showDownloadButton: true,\r\n      showDeleteButton: false,\r\n    };\r\n  },\r\n  methods: {\r\n    showDownloadButton() {\r\n      this.showDownloadButton = false;\r\n    },\r\n    showDeleteButton() {\r\n      this.showDeleteButton = true;\r\n    },\r\n\r\n    hideDeleteButton() {\r\n      this.showDeleteButton = false;\r\n    },\r\n    hideDownloadButton() {\r\n      this.showDownloadButton = true;\r\n    },\r\n    toggleFolder(folder) {\r\n      if (folder.type === \"folder\") {\r\n        if (this.expandedFolder === folder) {\r\n          this.expandedFolder = null;\r\n          this.$store.dispatch(\"setSelectedFolder\", folder);\r\n        } else {\r\n          this.expandedFolder = folder;\r\n          this.$store.dispatch(\"setSelectedFolder\", folder);\r\n        }\r\n        console.log(\r\n          \"this.$store.state.expandedFolder\",\r\n          this.$store.state.expandedFolder\r\n        );\r\n      }\r\n    },\r\n    toggleModal() {\r\n      this.modalVisible = !this.modalVisible;\r\n    },\r\n    handleCancel() {\r\n      this.showModal = false;\r\n      this.toggleCreateButtons();\r\n    },\r\n    toggleCreateButtons(folder) {\r\n      this.createMenuVisible = !this.createMenuVisible;\r\n      this.$store.dispatch(\"setSelectedFolder\", folder);\r\n    },\r\n    closeCreateButtons() {\r\n      this.createMenuVisible = false;\r\n    },\r\n    handleConfirmCreation(itemName, itemType) {\r\n      if (itemType === \"folder\") {\r\n        this.createFolder(itemName);\r\n      } else if (itemType === \"file\") {\r\n        this.createFile(itemName);\r\n        console.log(\"создали файл\");\r\n      }\r\n      this.showModal = false;\r\n      this.$store.dispatch(\"setSelectedFolder\", this.expandedFolder);\r\n      this.toggleCreateButtons();\r\n    },\r\n    createFolder() {\r\n      this.showModal = true;\r\n      this.modalPrompt = \"Введите название папки\";\r\n      this.modalType = \"folder\";\r\n    },\r\n    createFile() {\r\n      this.showModal = true;\r\n      this.modalPrompt = \"Введите название файла\";\r\n      this.modalType = \"file\";\r\n    },\r\n    handleCreate(newItem) {\r\n      this.item.children = this.item.children || [];\r\n      this.item.children.push(newItem);\r\n      this.expandedFolder = this.item;\r\n      this.showModal = false;\r\n    },\r\n    selectFile() {\r\n      console.log(\"BEFORE state.selectedFile\", this.$store.state.selectedFile);\r\n      this.$store.dispatch(\"setSelectedFile\", this.item);\r\n      console.log(\"after state.selectedFile\", this.$store.state.selectedFile);\r\n      console.log(\"this.item: \", this.item);\r\n    },\r\n    updateNewItemName(value) {\r\n      this.localNewItemName = value;\r\n      this.$emit(\"updateNewItemName\", value);\r\n    },\r\n    updateChildNewItemName(value) {\r\n      this.$emit(\"updateNewItemName\", value);\r\n    },\r\n    createFileInItem(item) {},\r\n    downloadFile() {\r\n      const content = this.item.content || \"\"; // Получаем содержимое файла\r\n      const blob = new Blob([content], { type: \"text/plain\" });\r\n      const link = document.createElement(\"a\");\r\n      const url = URL.createObjectURL(blob);\r\n\r\n      link.href = url;\r\n      link.download = this.item.name;\r\n      document.body.appendChild(link);\r\n      link.click();\r\n\r\n      document.body.removeChild(link);\r\n      window.URL.revokeObjectURL(url);\r\n    },\r\n    renameItem() {\r\n      if (this.item) {\r\n        this.startRename();\r\n      }\r\n    },\r\n    startRename() {\r\n      this.item.renaming = true;\r\n      this.item.newName = this.item.name;\r\n      this.$nextTick(() => {\r\n        const input = this.$refs.renameInput;\r\n        if (input) {\r\n          input.focus();\r\n        }\r\n      });\r\n    },\r\n\r\n    finishRename() {\r\n      if (this.item.newName.trim() !== \"\") {\r\n        this.item.name = this.item.newName;\r\n        this.item.renaming = false;\r\n        this.$store.dispatch(\"renameItem\", this.item);\r\n      } else {\r\n        // Handle empty name or cancel action\r\n        this.item.renaming = false;\r\n      }\r\n    },\r\n    deleteFile(item) {\r\n      if (item.type === \"file\") {\r\n        this.$store.commit(\"setSelectedFile\", this.item);\r\n      }\r\n      if (item.type === \"folder\") {\r\n        this.$store.commit(\"setSelectedFolder\", item);\r\n        this.$store.commit(\"deleteFile\");\r\n        console.log(\"хочу удалиться\");\r\n      }\r\n      if (this.$store.state.selectedFile) {\r\n        const selectedFileId = this.$store.state.selectedFile.id;\r\n        this.$store.commit(\"deleteFile\");\r\n        console.log(\"хочу удалиться\");\r\n        this.$store.commit(\"setSelectedFile\", null);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Add or adjust styles as needed */\r\n.folder .rename-input {\r\n  width: auto;\r\n}\r\n\r\n.file .rename-input {\r\n  text-align: left;\r\n  width: 125px;\r\n}\r\n\r\n.rename-input {\r\n  border: none;\r\n  padding: 0;\r\n  margin: 0;\r\n  font-size: inherit;\r\n  font-family: inherit;\r\n  outline: none;\r\n  background-color: transparent;\r\n  color: inherit;\r\n}\r\n.control {\r\n  position: relative;\r\n  display: flex;\r\n  jus
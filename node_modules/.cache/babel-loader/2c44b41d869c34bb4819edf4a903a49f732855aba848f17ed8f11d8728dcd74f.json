{"ast":null,"code":"import gsap from \"gsap\";\nimport RepositoryList from \"@/components/RepositoryList.vue\";\nimport Particles from \"@/components/Particles.vue\";\nexport default {\n  components: {\n    RepositoryList,\n    Particles\n  },\n  data() {\n    return {\n      titles: [\"Создавай\", \"Экспериментируй\", \"Генерируй\", \"Твори\", \"Погружайся\"],\n      currentTitle: \"\"\n    };\n  },\n  mounted() {\n    this.animateTitle();\n  },\n  methods: {\n    animateTitle() {\n      const titles = this.$refs.titles;\n      const description = this.$refs.description; // Добавляем ссылку на описание\n\n      const timeline = gsap.timeline();\n      this.titles.forEach((title, index) => {\n        timeline.to(titles, {\n          opacity: 1,\n          duration: 0.5,\n          onComplete: () => this.updateTitle(title)\n        }) // Исчезновение с движением вверх\n        .to(titles, {\n          opacity: 1,\n          duration: 0.5\n        }); // Появление с движением вниз\n      });\n\n      timeline.to(description, {\n        opacity: 1,\n        y: 0,\n        duration: 1\n      }, \"-=0.5\").to(titles, {\n        opacity: 1,\n        duration: 0.5\n      }); // Появление описания с задержкой\n    },\n\n    updateTitle(newTitle) {\n      this.currentTitle = newTitle;\n    }\n  }\n};","map":{"version":3,"names":["gsap","RepositoryList","Particles","components","data","titles","currentTitle","mounted","animateTitle","methods","$refs","description","timeline","forEach","title","index","to","opacity","duration","onComplete","updateTitle","y","newTitle"],"sources":["C:\\Users\\PC\\Desktop\\vue\\vue-code-collaborator\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"home-block\">\r\n      <router-link to=\"/\">Домой</router-link>\r\n      <router-link to=\"/repository\">Репозиторий</router-link>\r\n\r\n      <h1 ref=\"titles\" class=\"home__title\">{{ currentTitle }}</h1>\r\n      <p ref=\"description\" class=\"home__description\">Описание проекта</p>\r\n    </div>\r\n    <Particles />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport gsap from \"gsap\";\r\nimport RepositoryList from \"@/components/RepositoryList.vue\";\r\nimport Particles from \"@/components/Particles.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    RepositoryList,\r\n    Particles,\r\n  },\r\n  data() {\r\n    return {\r\n      titles: [\r\n        \"Создавай\",\r\n        \"Экспериментируй\",\r\n        \"Генерируй\",\r\n        \"Твори\",\r\n        \"Погружайся\",\r\n      ],\r\n      currentTitle: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.animateTitle();\r\n  },\r\n  methods: {\r\n    animateTitle() {\r\n      const titles = this.$refs.titles;\r\n      const description = this.$refs.description; // Добавляем ссылку на описание\r\n\r\n      const timeline = gsap.timeline();\r\n\r\n      this.titles.forEach((title, index) => {\r\n        timeline\r\n          .to(titles, {\r\n            opacity: 1,\r\n            duration: 0.5,\r\n            onComplete: () => this.updateTitle(title),\r\n          }) // Исчезновение с движением вверх\r\n          .to(titles, { opacity: 1, duration: 0.5 }); // Появление с движением вниз\r\n      });\r\n\r\n      timeline\r\n        .to(description, { opacity: 1, y: 0, duration: 1 }, \"-=0.5\")\r\n        .to(titles, { opacity: 1, duration: 0.5 }); // Появление описания с задержкой\r\n    },\r\n    updateTitle(newTitle) {\r\n      this.currentTitle = newTitle;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-block {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 40vw;\r\n  height: 100vh;\r\n  background-color: #2a2a32;\r\n}\r\n\r\n.home__description {\r\n  opacity: 0;\r\n}\r\n</style>\r\n"],"mappings":"AAcA,OAAOA,IAAG,MAAO,MAAM;AACvB,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,UAAU,EAAE;IACVF,cAAc;IACdC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN,UAAU,EACV,iBAAiB,EACjB,WAAW,EACX,OAAO,EA
{"ast":null,"code":"import gsap from 'gsap';\nimport RepositoryList from \"@/components/RepositoryList.vue\";\nimport Particles from \"@/components/Particles.vue\";\nexport default {\n  components: {\n    RepositoryList,\n    Particles\n  },\n  data() {\n    return {\n      titles: [\"Создавай\", \"Экспериментируй\", \"Генерируй\", \"Твори\", \"Погружайся\"],\n      currentTitle: \"\"\n    };\n  },\n  mounted() {\n    this.animateTitle();\n  },\n  methods: {\n    animateTitle() {\n      const titles = this.$refs.titles;\n      const description = this.$refs.description;\n      const timeline = gsap.timeline();\n      this.titles.forEach((title, index) => {\n        if (index === this.titles.length - 1) {\n          timeline.to(titles, {\n            opacity: 1,\n            y: 0,\n            duration: 0.5\n          }).to(titles, {\n            opacity: 0,\n            y: 100,\n            duration: 0.5,\n            onComplete: () => {\n              if (typeof this.updateTitle === 'function') {\n                this.updateTitle(title);\n              }\n            }\n          }, '+=0.5');\n        } else {\n          timeline.to(titles, {\n            opacity: 1,\n            duration: 0.5,\n            onComplete: () => {\n              if (typeof this.updateTitle === 'function') {\n                this.updateTitle(title);\n              }\n            }\n          }).to(titles, {\n            opacity: 1,\n            duration: 0.5\n          });\n        }\n      });\n      timeline.to(description, {\n        opacity: 1,\n        y: 0,\n        duration: 1\n      }, \"-=0.5\");\n    }\n  }\n};","map":{"version":3,"names":["gsap","RepositoryList","Particles","components","data","titles","currentTitle","mounted","animateTitle","methods","$refs","description","timeline","forEach","title","index","length","to","opacity","y","duration","onComplete","updateTitle"],"sources":["C:\\Users\\PC\\Desktop\\vue\\vue-code-collaborator\\src\\views\\Home.vue"],"sourcesContent
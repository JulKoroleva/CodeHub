{"ast":null,"code":"import gsap from 'gsap';\nimport RepositoryList from \"@/components/RepositoryList.vue\";\nimport Particles from \"@/components/Particles.vue\";\nexport default {\n  components: {\n    RepositoryList,\n    Particles\n  },\n  data() {\n    return {\n      titles: [\"Создавай\", \"Экспериментируй\", \"Генерируй\", \"Твори\", \"Погружайся\"],\n      currentTitle: \"\"\n    };\n  },\n  mounted() {\n    this.animateTitle();\n  },\n  methods: {\n    animateTitle() {\n      const titles = this.$refs.titles;\n      const timeline = gsap.timeline();\n      this.titles.forEach((title, index) => {\n        timeline.to(titles, {\n          autoAlpha: 0,\n          duration: 1,\n          onComplete: () => this.updateTitle(title)\n        }).to(titles, {\n          autoAlpha: 1,\n          duration: 1\n        });\n      });\n      timeline.to('.home__description', {\n        opacity: 1,\n        y: 0,\n        duration: 1\n      });\n    },\n    updateTitle(newTitle) {\n      this.currentTitle = newTitle;\n    }\n  }\n};","map":{"version":3,"names":["gsap","RepositoryList","Particles","components","data","titles","currentTitle","mounted","animateTitle","methods","$refs","timeline","forEach","title","index","to","autoAlpha","duration","onComplete","updateTitle","opacity","y","newTitle"],"sources":["C:\\Users\\PC\\Desktop\\vue\\vue-code-collaborator\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"home-block\">\r\n      <router-link to=\"/\">Домой</router-link>\r\n      <router-link to=\"/repository\">Репозиторий</router-link>\r\n\r\n      <h1 ref=\"titles\" class=\"home__title\">{{ currentTitle }}</h1>\r\n    </div>\r\n    <Particles />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport gsap from 'gsap';\r\nimport RepositoryList from \"@/components/RepositoryList.vue\";\r\nimport Particles from \"@/components/Particles.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    RepositoryList,\r\n    Particles,\r\n  },\r\n  data() {\r\n    return {\r\n      titles: [\"Создавай\", \"Экспериментируй\", \"Генерируй\", \"Твори\", \"Погружайся\"],\r\n      currentTitle: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.animateTitle();\r\n  },\r\n  methods: {\r\n    animateTitle() {\r\n      const titles = this.$refs.titles;\r\n\r\n      const timeline = gsap.timeline();\r\n\r\n      this.titles.forEach((title, index) => {\r\n        timeline.to(titles, { autoAlpha: 0, duration: 1, onComplete: () => this.updateTitle(title) })\r\n               .to(titles, { autoAlpha: 1, duration: 1 });\r\n      });\r\n\r\n      timeline.to('.home__description', { opacity: 1, y: 0, duration: 1 });\r\n    },\r\n    updateTitle(newTitle) {\r\n      this.currentTitle = newTitle;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-block {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 40vw;\r\n  height: 100vh;\r\n  background-color: #2a2a32;\r\n}\r\n\r\n.home__description {\r\n  opacity: 0;\r\n}\r\n</style>\r\n"],"mappings":"AAaA,OAAOA,IAAG,MAAO,MAAM;AACvB,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,UAAU,EAAE;IACVF,cAAc;IACdC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAAC,UAAU,EAAE,iBAAiB,EAAE,WAAW,EAAE,OAAO,EAAE,YAAY,CAAC;MAC3EC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACPD,YAAYA,CAAA,EAAG;MACb,MAAMH,MAAK,GAAI,IAAI,CAACK,KAAK,CAACL,MAAM;MAEhC,MAAMM,QAAO,GAAIX,IAAI,CAACW,QAAQ,CAAC,CAAC;MAEhC,IAAI,CAACN,MAAM,CAACO,OAAO,CAAC,CAACC,KAAK,EAAEC,KAAK,KAAK;QACpCH,QAAQ,CAACI,EAAE,CAACV,MAAM,EAAE;UAAEW,SAAS,EAAE,CAAC;UAAEC,QAAQ,EAAE,CAAC;UAAEC,UAAU,EAAEA,CAAA,KAAM,IAAI,CAACC,WAAW,CAACN,KAAK;QAAE,CAAC,EACpFE,EAAE,CAACV,MAAM,EAAE;UAAEW,SAAS,EAAE,CAAC;UAAEC,QAAQ,EAAE;QAAE,CAAC,CAAC;MACnD,CAAC,CAAC;MAEFN,QAAQ,CAACI,EAAE,CAAC,oBAAoB,EAAE;QAAEK,OAAO,EAAE,CAAC;QAAEC,CAAC,EAAE,CAAC;QAAEJ,QAAQ,EAAE;MAAE,CAAC,CAAC;IACtE,CAAC;IACDE,WAAWA,CAACG,QAAQ,EAAE;MACpB,IAAI,CAAChB,YAAW,GAAIgB,QAA
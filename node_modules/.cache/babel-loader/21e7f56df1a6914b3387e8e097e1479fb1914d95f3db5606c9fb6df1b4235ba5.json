{"ast":null,"code":"import gsap from \"gsap\";\nimport RepositoryList from \"@/components/RepositoryList.vue\";\nimport Particles from \"@/components/Particles.vue\";\nexport default {\n  components: {\n    RepositoryList,\n    Particles\n  },\n  data() {\n    return {\n      titles: [\"Создавай\", \"Экспериментируй\", \"Генерируй\", \"Твори\", \"Погружайся\"],\n      currentTitle: \"\"\n    };\n  },\n  mounted() {\n    this.animateTitle();\n  },\n  methods: {\n    updateTitle(newTitle) {\n      this.currentTitle = newTitle;\n    },\n    animateTitle() {\n      const titles = this.$refs.titles;\n      const description = this.$refs.description;\n      const timeline = gsap.timeline();\n      this.titles.forEach((title, index) => {\n        timeline.to(titles, {\n          opacity: 0,\n          y: -20,\n          // движение вверх\n          duration: 0.5,\n          onComplete: () => this.updateTitle(title)\n        });\n      });\n      timeline.to(titles, {\n        opacity: 1,\n        y: 0,\n        // движение вниз\n        duration: 0.5,\n        onComplete: () => {\n          // По завершению анимации заголовка\n          gsap.to(description, {\n            opacity: 0,\n            scale: 0.8,\n            duration: 0.5,\n            onComplete: () => {\n              // По завершению анимации описания\n              this.updateDescription(); // Передай функцию обновления описания\n              gsap.to(description, {\n                opacity: 1,\n                scale: 1,\n                duration: 0.5\n              });\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["gsap","RepositoryList","Particles","components","data","titles","currentTitle","mounted","animateTitle","methods","updateTitle","newTitle","$refs","description","timeline","forEach","title","index","to","opacity","y","duration","onComplete","scale","updateDescription"],"sources":["C:\\Users\\PC\\Desktop\\vue\\vue-code-collaborator\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"home-block\">\r\n      <router-link to=\"/\">Домой</router-link>\r\n      <router-link to=\"/repository\">Репозиторий</router-link>\r\n\r\n      <h1 ref=\"titles\" class=\"home__title\">{{ currentTitle }}</h1>\r\n      <p ref=\"description\" class=\"home__description\">Описание проекта</p>\r\n    </div>\r\n    <Particles />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport gsap from \"gsap\";\r\nimport RepositoryList from \"@/components/RepositoryList.vue\";\r\nimport Particles from \"@/components/Particles.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    RepositoryList,\r\n    Particles,\r\n  },\r\n  data() {\r\n    return {\r\n      titles: [\r\n        \"Создавай\",\r\n        \"Экспериментируй\",\r\n        \"Генерируй\",\r\n        \"Твори\",\r\n        \"Погружайся\",\r\n      ],\r\n      currentTitle: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.animateTitle();\r\n  },\r\n  methods: {\r\n     updateTitle(newTitle) {\r\n    this.currentTitle = newTitle;\r\n  },\r\n   animateTitle() {\r\n  const titles = this.$refs.titles;\r\n  const description = this.$refs.description;\r\n\r\n  const timeline = gsap.timeline();\r\n\r\n  this.titles.forEach((title, index) => {\r\n    timeline.to(titles, {\r\n      opacity: 0,\r\n      y: -20, // движение вверх\r\n      duration: 0.5,\r\n      onComplete: () => this.updateTitle(title),\r\n    });\r\n  });\r\n\r\n  timeline.to(titles, {\r\n    opacity: 1,\r\n    y: 0, // движение вниз\r\n    duration: 0.5,\r\n    onComplete: () => {\r\n      // По завершению анимации заголовка\r\n      gsap.to(description, {\r\n        opacity: 0,\r\n        scale: 0.8,\r\n        duration: 0.5,\r\n        onComplete: () => {\r\n          // По завершению анимации описания\r\n          this.updateDescription(); // Передай функцию обновления описания\r\n          gsap.to(description, { opacity: 1, scale: 1, duration: 0.5 });\r\n        },\r\n      });\r\n    },\r\n  });\r\n}\r\n\r\n\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-block {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 40vw;\r\n  height: 100vh;\r\n  background-color: #2a2a32;\r\n}\r\n\r\n.home__description {\r\n  opacity: 0;\r\n}\r\n</style>\r\n"],"mappings":"AAcA,OAAOA,IAAG,MAAO,MAAM;AACvB,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,UAAU,EAAE;IACVF,cAAc;IACdC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CACN,UAAU,EACV,iBAAiB,EACjB,WAAW,EACX,OAAO,EACP,YAAY,CACb;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACNC,WAAWA,CAACC,QAAQ,EAAE;MACvB,IAAI,CAACL,YAAW,GAAIK,QAAQ;IAC9B,CAAC;IACAH,YAAYA,CAAA,EAAG;MAChB,MAAMH,MAAK,GAAI,IAAI,CAACO,KAAK,CAACP,MAAM;MAChC,MAAMQ,WAAU,GAAI,IAAI,CAACD,KAAK,CAACC,WAAW;MAE1C,MAAMC,QAAO,GAAId,IAAI,CAACc,QAAQ,CAAC,CAAC;MAEhC,IAAI,CAACT,MAAM,CAACU,OAAO,CAAC,CAACC,KAAK,EAAEC,KAAK,KAAK;QACpCH,QAAQ,CAACI,EAAE,CAACb,MAAM,EAAE;UAClBc,OAAO,EAAE,CAAC;UACVC,CAAC,EAAE,CAAC,EAAE;UAAE;UACRC,QAAQ,EAAE,GAAG;UACbC,UAAU,EAAEA,CAAA,KAAM,IAAI,CAACZ,WAAW,CAACM,KAAK;QAC1C,CAAC,CAAC;MACJ,CAAC,CAAC;MAEFF,QAAQ,CAACI,EAAE,CAACb,MAAM,EAAE;QAClBc,OAAO,EAAE,CAAC;QACVC,CAAC,EAAE,CAAC;QAAE;QACNC,QAAQ,EAAE,GAAG;QACbC,UAAU,EAAEA,CAAA,KAAM;UAChB;UACAtB,IAAI,CAACkB,EAAE,CAACL,WAAW,EAAE;YACnBM,OAAO,EAAE,CAAC;YACVI,KAAK,EAAE,GAAG;YACVF,QAAQ,EAAE,GAAG;YACbC,UAAU,EAAEA,CAAA,KAAM;cAChB;cACA,IAAI,CAACE,iBAAiB,CAAC,CAAC,EAAE;cAC1BxB,IAAI,CAACkB,EAAE,CAACL,WAAW,EAAE;gBAAEM,OAAO,EAAE,CAAC;gBAAEI,KAAK,EAAE,CAAC;gBAAEF,QAAQ,EAAE;cAAI,CAAC,CAAC;YAC/D;UACF,CAAC,CAAC;QACJ;MACF,CAAC,CAAC;IACJ;EAIE;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}